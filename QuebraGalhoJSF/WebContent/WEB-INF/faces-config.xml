<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
	<managed-bean>
		<managed-bean-name>index</managed-bean-name>
		<managed-bean-class>controller.Index</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>favYear3Value</property-name>
			<property-class>quebragalhows.ProfissaoVO[]</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<to-view-id>/resultadoBusca.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
